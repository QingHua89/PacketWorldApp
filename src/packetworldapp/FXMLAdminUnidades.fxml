<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="packetworldapp.FXMLAdminUnidadesController">
    <children>
        <Pane prefHeight="500.0" prefWidth="800.0">
         <children>
            <ImageView fitHeight="500.0" fitWidth="800.0" pickOnBounds="true">
               <image>
                  <Image url="@recursos/Fondo-Largo2.png" />
               </image></ImageView>
            <Pane prefHeight="65.0" prefWidth="800.0" style="-fx-background-color: #023047;">
               <children>
                  <ImageView fx:id="ivAtras" fitHeight="35.0" fitWidth="25.0" layoutX="14.0" layoutY="15.0" onMouseClicked="#ClicAtras" pickOnBounds="true">
                     <image>
                        <Image url="@recursos/back.png" />
                     </image>
                  </ImageView>
                  <Label layoutX="53.0" layoutY="17.0" text="Unidades" textFill="WHITESMOKE">
                     <font>
                        <Font name="System Bold" size="22.0" />
                     </font></Label>
               </children></Pane>
            <TextField fx:id="tfBuscar" layoutX="37.0" layoutY="98.0" prefHeight="29.0" prefWidth="191.0" promptText="VIN/Marca/NII" />
            <Button layoutX="228.0" layoutY="98.0" mnemonicParsing="false" onAction="#ClicBuscar" prefHeight="29.0" prefWidth="51.0" text="Buscar" />
            <Button layoutX="400.0" layoutY="98.0" mnemonicParsing="false" onAction="#ClicNuevo" prefHeight="29.0" prefWidth="100.0" text="Nuevo" />
            <Button layoutX="539.0" layoutY="98.0" mnemonicParsing="false" onAction="#ClicModificar" prefHeight="29.0" prefWidth="100.0" text="Modificar" />
            <Button layoutX="670.0" layoutY="98.0" mnemonicParsing="false" onAction="#ClicDarBaja" prefHeight="29.0" prefWidth="100.0" text="Dar de Baja" />
            <TableView fx:id="tvUnidades" layoutX="30.0" layoutY="191.0" prefHeight="285.0" prefWidth="740.0">
               <columns>
                  <TableColumn fx:id="tcMarca" prefWidth="70.0" text="Marca" />
                  <TableColumn fx:id="tcModelo" prefWidth="70.0" text="Modelo" />
                  <TableColumn fx:id="tcAnio" prefWidth="70.0" text="Año" />
                  <TableColumn fx:id="tcVin" prefWidth="148.0" text="VIN" />
                  <TableColumn fx:id="tcTipo" prefWidth="84.0" text="Tipo" />
                  <TableColumn fx:id="tcNII" prefWidth="100.0" text="Número Interno" />
                  <TableColumn fx:id="tcEstatus" prefWidth="95.0" text="Estatus" />
                  <TableColumn fx:id="tcBaja" prefWidth="102.0" text="Motivo de Baja" />
               </columns>
            </TableView>
         </children></Pane>
    </children>
</AnchorPane>
