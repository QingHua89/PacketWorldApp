<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="packetworldapp.FXMLFormularioSucursalesController">
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0">
         <children>
            <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true">
               <image>
                  <Image url="@recursos/Fondo-Largo2.png" />
               </image>
            </ImageView>
            <Pane prefHeight="45.0" prefWidth="600.0" style="-fx-background-color: #023047;">
               <children>
                  <Label layoutX="26.0" layoutY="9.0" text="Formulario Sucursal" textFill="#f5f5f5">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
               </children></Pane>

            <Label layoutX="32.0" layoutY="72.0" text="Código Sucursal" />

            <TextField fx:id="tfCodSucursal" layoutX="32.0" layoutY="89.0" promptText="PW-0" />

            <Label layoutX="223.0" layoutY="72.0" text="Nombre" />

            <TextField fx:id="tfNombre" layoutX="223.0" layoutY="89.0" promptText="Packet World Sucursal" />

            <Label layoutX="413.0" layoutY="72.0" text="Estatus" />

            <TextField fx:id="tfEstatus" layoutX="413.0" layoutY="89.0" promptText="García" text="Activa" />
            <Separator layoutX="22.0" layoutY="139.0" prefWidth="550.0" />
            <Label layoutX="31.0" layoutY="149.0" text="Dirección:">
               <font>
                  <Font size="13.0" />
               </font>
            </Label>
            <Label layoutX="34.0" layoutY="179.0" text="Calle" />
            <TextField fx:id="tfCalle" layoutX="34.0" layoutY="196.0" promptText="Calle" />
            <Label layoutX="225.0" layoutY="179.0" text="Número" />
            <TextField fx:id="tfNumero" layoutX="225.0" layoutY="196.0" promptText="1A" />
            <Label layoutX="415.0" layoutY="179.0" text="Colonia" />
            <TextField fx:id="tfColonia" layoutX="415.0" layoutY="196.0" promptText="Colonia" />
            <Label layoutX="34.0" layoutY="248.0" text="Código Postal" />
            <TextField fx:id="tfCodigoPostal" layoutX="34.0" layoutY="265.0" promptText="C.P." />
            <Label layoutX="225.0" layoutY="248.0" text="Ciudad" />
            <TextField fx:id="tfCiudad" layoutX="225.0" layoutY="265.0" promptText="Ciudad" />
            <Label layoutX="415.0" layoutY="248.0" text="Estado" />
            <TextField fx:id="tfEstado" layoutX="415.0" layoutY="265.0" promptText="Estado" />

            <Button fx:id="btGuardar" layoutX="467.0" layoutY="352.0" mnemonicParsing="false" onAction="#clicBtnGuardar" prefHeight="27.0" prefWidth="100.0" text="Guardar" />

            <Button fx:id="btCancelar" layoutX="375.0" layoutY="352.0" mnemonicParsing="false" onAction="#clicBtnCancelar" prefHeight="27.0" prefWidth="75.0" text="Cancelar" />
         </children>
      </Pane>
   </children>
</AnchorPane>
